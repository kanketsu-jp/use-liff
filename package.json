    "@types/react": "^18.2.67",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanketsu-jp/use-liff"
  },
  "bugs": {
    "url": "https://github.com/kanketsu-jp/use-liff/issues"
